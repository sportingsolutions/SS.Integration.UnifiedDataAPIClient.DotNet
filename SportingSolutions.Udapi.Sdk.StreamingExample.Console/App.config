<?xml version="1.0"?>
<configuration>
  <appSettings>
    <add key="ss.user" value="{username here}"/>
    <add key="ss.password" value="{password here}"/>
    <add key="ss.url" value="{url here}"/>
    <add key="ss.newFixtureCheckerFrequency" value="60000"/>
  </appSettings>
  <system.serviceModel>
    <bindings>
      <webHttpBinding>
        <binding name="webBinding" sendTimeout="00:01:00">
          <security mode="None"/>
        </binding>
      </webHttpBinding>
    </bindings>
  </system.serviceModel>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/></startup></configuration>
